{
    "name": "contenttype-id-generator",
    "displayName": "contenttype-id-generator",
    "description": "Generates SharePoint ContentTypesStringIDs",
    "publisher": "Ole Bergtun",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.SPOleB"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.SPOleB",
                "title": "SPO contenttype stringids"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/clipboardy": "^1.1.0",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "clipboardy": "^1.2.3",
        "guid-typescript": "^1.0.9"
    }
}
